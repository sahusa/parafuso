{
    "version": 2,
    "builds":[
        { 
            "src": "index.js", 
            "use": "@now/node-server",
            "config": {
                "includeFiles": [
                    "public/**",
                    "views/**"
                ]
            }
        }
    ],
    "routes":[
        { "src": "/.*", "dest": "index.js" }
    ],
    "alias": "www.parafusomodapraia.com.br"       
}